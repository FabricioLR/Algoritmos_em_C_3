Algoritmo "comparar_inteiros"
Var
   int1, int2: real
Inicio
      escreva("Digite o valor de um inteiro: ")
      leia(int1)
      escreva("Digite o valor de outro inteiro: ")
      leia(int2)

      se (int1 = int2) entao
         escreval("Os inteiros sao iguais")
      senao
         escreval("Os inteiros sao diferentes")
      fimse
Fimalgoritmo